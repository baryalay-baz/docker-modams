@if (TempData["success"] != null)
    {
        <script>
        @*notif({
            msg: "@TempData["success"]",
            type: "success",
            position: "right",
            time: 1000
        });*@
        notif({
            type: "primary",
            msg: "<b>Success: </b>@TempData["success"]",
            position: "center",
            width: 500,
            height: 60,
            autohide: true
        });
        </script>
    }
    @if (TempData["error"] != null)
    {
        <script>
        notif({
            type: "error",
            msg: "<b>Error: </b>@TempData["error"]",
            position: "center",
            width: 500,
            height: 60,
            autohide: true
        });
        @*notif({
            msg: "@TempData["error"]",
            type: "error",
            position: "right",
            time: 1000
        });*@
        </script>
    }